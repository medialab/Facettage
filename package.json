{
  "name": "facettage",
  "version": "1.0.0",
  "description": "Backendless facet management",
  "main": "facettage.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "npm run transpile && npm run minify",
    "minify": "uglifyjs ./facettage.js -o ./facettage.min.js",
    "transpile": "babel --presets es2015 ./src/facettage.js -o ./facettage.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medialab/Facettage.git"
  },
  "keywords": [
    "backendless",
    "facet",
    "datascape"
  ],
  "author": "medialab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medialab/Facettage/issues"
  },
  "homepage": "https://github.com/medialab/Facettage#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "uglifyjs": "^2.4.10"
  }
}
